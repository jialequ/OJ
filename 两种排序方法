#include <iostream>
#include <string>
#include <vector>
using namespace std;

int main()
{
	int n;
	cin >> n;
	vector<string> v;
	v.resize(n);
	bool flag1 = false;
	bool flag2 = false;
	for (int i = 0; i < n; i++)
	{
		cin >> v[i];
	}
	for (int i = 1; i < n; i++)
	{
		if (v[i] >= v[i - 1])
			flag1 = true;
		else if (v[i] < v[i - 1])
		{
			flag1 = false;
			break;
		}
	}
	for (int i = 1; i < n; i++)
	{
		if (v[i].size() >= v[i - 1].size())
			flag2 = true;
		else
		{
			flag2 = false;
			break;
		}
	}
	if (flag1 && flag2)
		cout << "both" << endl;
	else if (!flag1 && !flag2)
		cout << "none" << endl;
	else if (flag1)
		cout << "lexicographically" << endl;
	else if (flag2)
		cout << "lengths" << endl;
	return 0;
}
